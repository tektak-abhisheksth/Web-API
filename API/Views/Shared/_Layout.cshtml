@using System
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/iLoop")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/Content/css")
    <meta name="description" content="iLoop Web API Page" />
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top" style="background-color: #0779bb">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @*@Html.ActionLink("iLoop", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })*@
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav" style="width: 100%;">
                    <li>
                        <a href="@Url.Action("Index", "Home", new {area = ""})"><img title="Home" onclick="document.location.href='@Url.Action("Index", "Home", new {area = ""})'" src="data:image/png;base64," style="vertical-align: central" width="40" height="20" alt="iLoop" /></a>
                    </li>
                    <li>@Html.ActionLink("Log", "Index", "Home", new { area = "" }, new { @id = "indexLog", title = "Change announcements (Alt+G)." })</li>
                    <li>@Html.ActionLink("API", "Index", "Help", new { area = "" }, new { @id = "indexApi", title = "List of APIs currently in deployment (Alt+A)." })</li>
                    <li>@Html.ActionLink("Dev", "Dev", "Home", new { area = "" }, new { @id = "indexDev", title = "Technical notes and directions to developers (Alt+D)." })</li>
                    <li>@Html.ActionLink("Conventions", "Conventions", "Home", new { area = "" }, new { @id = "indexConvention", title = "Conventions and their meanings (Alt+C)." })</li>
                    <li>@Html.ActionLink("Trace", "Trace", "Home", new { area = "" }, new { @id = "indexTrace", title = "Tracing of all requests made to the server (Alt+R)." })</li>
                    <li>@Html.ActionLink("FAQs", "Faqs", "Home", new { area = "" }, new { @id = "indexFAQs", title = "Frequently asked questions (Alt+Q)." })</li>
                    <li style="float: right">
                        <span style="vertical-align: central; display: inline-block; margin-top: 15px">
                            <a id="indexSettings" style="margin-right: 10px" href="@Url.Action("Settings", "Home", new {area = ""})"><img title="System-wide Settings (Alt+N)." src="data:image/png;base64," style="vertical-align: central" width="20" height="20" alt="Settings" /></a>
                            @Html.Partial("_DataSetterPanel")
                        </span>
                    </li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <div style="width: 100%; text-align: center">
                &copy; @DateTime.Now.Year - <a href="http://www.tektak.com/">TekTak Nepal Pvt. Ltd.</a>
            </div>
        </footer>
    </div>
    @RenderSection("scripts", false)
    <div id="iLoopAPITip" class="tip">
        <span style="margin: 0 5px 0 5px;"></span>
        <input type="hidden" id="currentIndex" value="" />
    </div>
    @{const int imgSize = 30;}
    <div id="sidebar" style="position: fixed; right: 4%; top: 43%; width: 10px">
        <table style="width: 100%; border-collapse: separate; border-spacing: 0 5px;">
            <tr>
                <td><a id="pageFirst" href="#" title="Page top."><img width="@imgSize" height="@imgSize" title="Page top (A)." alt="Page top." src="data:image/png;base64," /></a></td>
            </tr>
            <tr>
                <td><a href="#" id="openAll" title="Expand all."><img width="@imgSize" height="@imgSize" title="Expand all (S)." alt="Expand all." src="data:image/png;base64," /></a></td>
            </tr>
            <tr>
                <td><a href="#" id="closeAll" title="Collapse all."><img width="@imgSize" height="@imgSize" title="Collapse all (D)." alt="Collapse all." src="data:image/png;base64," /></a></td>
            </tr>
            <tr>
                <td><a id="pageLast" href="#" title="Page bottom."><img width="@imgSize" height="@imgSize" title="Page bottom (F)." alt="Page bottom." src="data:image/png;base64," /></a></td>
            </tr>
        </table>
    </div>
    <script>
        function showTip(data, displayTipSecond) {
            if (data.length <= 0 || data.tip <= 0)
                return;
            var $tip = $("#iLoopAPITip");
            if ($tip.is(":visible"))
                return;
            $("#currentIndex").val(data.hasOwnProperty('index') ? data.index : 0);
            $tip.find("span").html(data.tip);
            $tip.fadeIn("slow");

            setTimeout(function () {
                $tip.fadeOut("slow");
            }, displayTipSecond * 1000);
        }

        var getTip = function (noServerCall) {
            var intervalMinute = 5;
            var displayTipSecond = 20;

            var index = $("#currentIndex").val();
            if (isNaN(index)) {
                $("#currentIndex").val(0);
            } else {
                if (!noServerCall)
                    $.ajax({
                        url: "@Url.Action("GetTip", "Home", new {area = ""})",
                        contentType: "application/json; charset=utf-8",
                        data: "index=" + index,
                        dataType: "json",
                        success: function (data) {
                            showTip(data, displayTipSecond);
                        },
                        error: function (e) {
                            console.log(e);
                        }
                    });
            }
            var now = new Date();
            var time = (intervalMinute - (now.getMinutes() % intervalMinute)) * 60 - now.getSeconds() - (now.getMilliseconds() / 1000);
            setTimeout(getTip, time * 1000);
        };

        getTip(true);

        function scrollPage(top) {
            if (top) {
                $("html, body").animate({ scrollTop: 0 }, { duration: 1000, queue: false });
                $("html, body").focus();
            } else {
                $("html, body").animate({ scrollTop: $(document).height() }, { duration: 1000, queue: false });
                $("html, body").focus();
                //$('html, body').animate({scrollTop: $('footer').offset().top}, 'slow');
            }
        }

        $('#pageFirst').click(function (event) {
            event.preventDefault();
            scrollPage(true);
        });

        $('#pageLast').click(function (event) {
            event.preventDefault();
            scrollPage(false);
        });

        function openAll() {
            if (typeof units !== 'undefined') {
                for (var i = 0; i < units.length; i++)
                    if (units[i].collapsible('collapsed'))
                        units[i].collapsible('open');
            }
            $("html, body").focus();
        }

        function closeAll() {
            if (typeof units !== 'undefined') {
                for (var i = 0; i < units.length; i++)
                    //if (!units[i].collapsible('collapsed'))
                    units[i].collapsible('close');
            }
            $("html, body").focus();
        }

        $('#closeAll').click(function (event) {
            event.preventDefault();
            closeAll();
            if (typeof showHide == 'function') {
                $('[name=displayArrow]').each(function (i, d) {
                    showHide(d, false);
                });
            }
        });

        $('#openAll').click(function (event) {
            event.preventDefault();
            openAll();
            if (typeof showHide == 'function') {
                $('[name=displayArrow]').each(function (i, d) {
                    showHide(d, true);
                });
            }
        });

        //$('#openAll').on("click", openAll);
        //$('#closeAll').on("click", closeAll);

        function setDisplay(needAnim, persist, clkType) {
            if ($("#chkColorCode").is(":checked"))
                persist = false;
            if (clkType === 0 || clkType === 1) {
                var webDisplay = ($.cookie('display.web') || 'true') == 'true';
                if (webDisplay)
                    $('[marktype=1]').each(function () {
                        animIn($(this), needAnim, persist, "#E0F5FF");
                    });
                else
                    $('[marktype=1]').each(function () {
                        if (needAnim) animOut($(this));
                        else $(this).hide();
                    });
            }

            if (clkType === 0 || clkType === 2) {
                var mobileDisplay = ($.cookie('display.mobile') || 'true') == 'true';
                if (mobileDisplay)
                    $('[marktype=2]').each(function () { animIn($(this), needAnim, persist, "#FFE6FF"); });
                else
                    $('[marktype=2]').each(function () {
                        if (needAnim) animOut($(this));
                        else $(this).hide();
                    });
            }

            if (clkType === 3) {
                if ($("#chkColorCode").is(":checked")) {
                    $(".notice").each(function () { $(this).css("background-color", "#FFFFFF"); });
                    $("#colorLegends").hide();
                }
                else {
                    $(".notice[marktype=1]").each(function () { $(this).css("background-color", "#E0F5FF"); });
                    $(".notice[marktype=2]").each(function () { $(this).css("background-color", "#FFE6FF"); });
                    $(".notice[marktype=3]").each(function () { $(this).css("background-color", "#FFFFD8"); });
                    $("#colorLegends").show();
                }
            }

            if (typeof processNew == 'function')
                processNew();
        }

        function persistDisplay(clkType) {
            $("#wrapper").mouseleave();
            var elem = $("#chkWebSpecific");
            $.cookie('display.web', elem.is(':checked').toString(), { expires: 365, path: '/' });
            elem = $("#chkMobileSpecific");
            $.cookie('display.mobile', elem.is(':checked').toString(), { expires: 365, path: '/' });
            elem = $("#chkColorCode");
            $.cookie('display.colors', (!elem.is(':checked')).toString(), { expires: 365, path: '/' });
            setDisplay(true, true, clkType);
        }

        $(function () {
            var webDisplay = ($.cookie('display.web') || 'true') === 'true';
            var mobileDisplay = ($.cookie('display.mobile') || 'true') === 'true';
            var colorDisplay = Boolean(($.cookie('display.colors') || 'true') === 'true');
            if (!colorDisplay) {
                $(".notice").each(function () { $(this).css("background-color", "#FFFFFF"); });
                $("#colorLegends").hide();
            }

            $('#chkWebSpecific').prop('checked', webDisplay);
            $('#chkMobileSpecific').prop('checked', mobileDisplay);
            $('#chkColorCode').prop('checked', !colorDisplay);

            $('#chkWebSpecific').click(function () {
                persistDisplay(1);
            });
            $('#chkMobileSpecific').click(function () {
                persistDisplay(2);
            });
            $('#chkColorCode').click(function () {
                persistDisplay(3);
            });

            $("div[marktype]").each(function () {
                $(this).prepend($('<span class="markLogo" style="width: 20px; height: 20px; position: absolute; right: 0;"></span>'));
            });

            $("td.enum-description").each(function () {
                $(this).prepend($('<span class="markLogo" style="width: 20px; height: 20px; position: absolute; right: 10px;"></span>'));
            });

            $("td.api-documentation").each(function () {
                $(this).prepend($('<span class="markToken" style="width: 20px; height: 20px; float: right;"></span><span class="markLogo" style="width: 20px; height: 20px; float: right;"></span>'));
            });

            //$("div.notice[markType='1'] > span[class='markLogo']").each(function () {
            //    $(this).attr("title", "Represents a web-only entry.");
            //});

            //$("div.notice[markType='2'] > span[class='markLogo']").each(function () {
            //    $(this).attr("title", "Represents a mobile-only entry.");
            //});

            setDisplay(false, false, 0);
        });

        $(window).load(function () {
            if ($.fn.collapsible)
                $('.collapsible').collapsible({});
            shortcut.add("A", function () {
                $('#pageFirst').click();
            }, { 'disable_in_input': true });
            shortcut.add("S", function () {
                $('#openAll').click();
            }, { 'disable_in_input': true });
            shortcut.add("D", function () {
                $('#closeAll').click();
            }, { 'disable_in_input': true });
            shortcut.add("F", function () {
                $('#pageLast').click();
            }, { 'disable_in_input': true });

            shortcut.add("Alt+G", function () {
                $('#indexLog')[0].click();
            }, { 'disable_in_input': true });
            shortcut.add("Alt+A", function () {
                $('#indexApi')[0].click();
            }, { 'disable_in_input': true });
            shortcut.add("Alt+D", function () {
                $('#indexDev')[0].click();
            }, { 'disable_in_input': true });
            shortcut.add("Alt+C", function () {
                $('#indexConvention')[0].click();
            }, { 'disable_in_input': true });
            shortcut.add("Alt+R", function () {
                $('#indexTrace')[0].click();
            }, { 'disable_in_input': true });
            shortcut.add("Alt+Q", function () {
                $('#indexFAQs')[0].click();
            }, { 'disable_in_input': true });
            shortcut.add("Alt+N", function () {
                $('#indexSettings')[0].click();
            }, { 'disable_in_input': true });
        });
    </script>
</body>
</html>
