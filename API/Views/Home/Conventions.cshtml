@using System
@using System.Linq
@using Model.Types
@using Utility
@model System.Collections.Generic.IEnumerable<API.Areas.HelpPage.ModelDescriptions.EnumTypeModelDescription>
<link href="~/Areas/HelpPage/HelpPage.css" rel="stylesheet" />
<div class="jumbotron" style="box-shadow: 0 5px 8px rgba(0, 0, 0, 0.5);">
    <h1 class="engraved" style="color: #B2B2B2">Conventions</h1>
</div>
    The lists shows the conventions that system server follows, as of
<span style="font-weight: bold" id="maxDate">@DateTime.UtcNow.ToLongDateString()</span><span style="font-weight: bold" id="newCount"></span>. The entries are subject to modification and/or addition.
<br /><br />
<div class="SystemlistStyle2">
    <!--#region Error Messages-->
    @{
        var description = "General error messages that are returned by iLoop system to service requester in case of client/server errors.";
        var dDay = new DateTime(2014, 8, 1);
        var size = 32;
    }
    <div class="collapsible" added="@dDay" id="errormessages" title="Added: @dDay.ToLongDateString()">
        Error Messages<span></span>
    </div>
    <div class="container" style="display: none">
        <br />
        <div style="margin-top: .5em">
            <strong>@description</strong>
        </div>
        <p>
            @{
                var oClassType = typeof(SystemResponseMessage);
                var oClassProperties = oClassType.GetProperties();
                var lst = oClassProperties.ToDictionary(prop => prop.Name, prop => prop.GetValue(oClassProperties).ToString());
            }
            <ul>
                @foreach (var item in lst)
                {
                    <li>
                        <strong> @item.Key.AddSpaceBeforeCapitalLetters()</strong>
                        <span> : @item.Value</span>
                    </li>
                }
            </ul>
            Specific validation error messages will be displayed along with these errors, with HTTP status codes of 4xx.
        </p>
    </div>
    <br />
    <!--#endregion-->
    <!--#region Legends-->
    @{
        description = "Symbols and their meanings.";
        dDay = new DateTime(2015, 6, 1);
    }
    <div class="collapsible" added="@dDay" id="symbols" title="Added: @dDay.ToLongDateString()">
        Legends<span></span>
    </div>
    <div class="container" style="display: none">
        <br />
        <div style="margin-top: .5em">
            <strong>@description</strong>
        </div>
        <p>
            <table style="margin-left: 12px; border-spacing: 10px; border-collapse: separate; width: 98%">
                <tr>
                    <td><img src="data:image/png;base64," width="@size" height="@size" /></td>
                    <td style="width:1%;white-space:nowrap;"><strong>Authorization required</strong> :</td>
                    <td>Access to the API requires user to include a valid Authorization header in the request (see @Html.ActionLink("Dev Notes", "Dev", null, null, null, "authorization", null, null)).</td>
                </tr>
                <tr>
                    <td><img src="data:image/png;base64," width="@size" height="@size" /></td>
                    <td style="width: 1%; white-space: nowrap;"><strong>Temporary token required</strong> :</td>
                    <td>Access to the API requires user to include a valid and unexpired temporary token (requested from the system itself) in the Authorization header of the request (see @Html.ActionLink("Dev Notes", "Dev", null, null, null, "temptoken", null, null)).</td>
                </tr>
                <tr>
                    <td><img src="data:image/png;base64," width="@size" height="@size" /></td>
                    <td style="width: 1%; white-space: nowrap;"><strong>Web-only element</strong> :</td>
                    <td>Designates an element/feature/note to be specific for web.</td>
                </tr>
                <tr>
                    <td><img src="data:image/png;base64," width="@size" height="@size" /></td>
                    <td style="width: 1%; white-space: nowrap;"><strong>Mobile-only element</strong> :</td>
                    <td>Designates an element/feature/note to be specific for mobile.</td>
                </tr>
                <tr>
                    <td><img src="~/Images/Android.png" width="@size" height="@size" /></td>
                    <td style="width: 1%; white-space: nowrap;"><strong>Android-only element</strong> :</td>
                    <td>Designates an element/feature/note to be specific for Android.</td>
                </tr>
                <tr>
                    <td><img src="~/Images/Apple.png" width="@size" height="@size" /></td>
                    <td style="width: 1%; white-space: nowrap;"><strong>iOS-only element</strong> :</td>
                    <td>Designates an element/feature/note to be specific for iOS.</td>
                </tr>
                <tr>
                    <td><img src="data:image/png;base64," width="@size" height="@size" /></td>
                    <td style="width: 1%; white-space: nowrap;"><strong>Correct Completion</strong> :</td>
                    <td>Indicates an operation completing successfully.</td>
                </tr>
                <tr>
                    <td><img src="data:image/png;base64," width="@size" height="@size" /></td>
                    <td style="width: 1%; white-space: nowrap;"><strong>Completion with complication</strong> :</td>
                    <td>Indicates an operation completing with one or more complications, usually due to mistakes made from client.</td>
                </tr>
                <tr>
                    <td><img src="data:image/png;base64," width="@size" height="@size" /></td>
                    <td style="width: 1%; white-space: nowrap;"><strong>Completion with error</strong> :</td>
                    <td>Indicates an operation not being completed successfully, usually due to one or more server errors.</td>
                </tr>
                <tr>
                    <td><img src="data:image/png;base64," width="@size" height="@size" /></td>
                    <td style="width: 1%; white-space: nowrap;"><strong>Synchronize</strong> :</td>
                    <td>Synchronize and/or update information from the server.</td>
                </tr>
                <tr>
                    <td style="background-color: #D1E8FF"><img src="data:image/png;base64," width="@size" height="@size" /></td>
                    <td style="width: 1%; white-space: nowrap;"><strong>Correct user information</strong> :</td>
                    <td>Corrects the user information provided in the sample, to help prevent finding/entering common information repeatedly.</td>
                </tr>
                <tr>
                    <td><img src="data:image/png;base64," width="@size" height="@size" /></td>
                    <td style="width:1%;white-space:nowrap;"><strong>New information</strong> :</td>
                    <td>Indicates a new entry, less than a week old.</td>
                </tr>
            </table>
        </p>
    </div>
    <!--#endregion-->
    <hr />
    @foreach (var convention in Model)
    {
        @Html.Partial("_ConventionTypeDescription", convention)
        <br />
    }
</div>
<p>&nbsp;</p>
<div>
    Please report any bugs and/or changes to <a title="If your browser is setup for email, clicking here will let you send us an email." href="mailto:indira.sapkota@tektak.com?subject= About Web API&amp;cc=Abhisheksth@tektak.com">Web API team</a>.
</div>
<p>&nbsp;</p>
@{ Disqus.Initialize("iloopwebapi"); }
@Disqus.ShowComments("conventions")
<script type="text/javascript">
    var units = [];
    $(function () {
        $('.collapsible[added]').each(function (i, d) {
            units.push($(d));
        });
    });

    function processNew() {
        $('.collapsible[added]').each(function (i, d) {
            $(d).removeClass('imgtag');
            var dtCurrent = new Date('@DateTime.Now.ToString("yyyy MM dd")');
            var dtRecord = new Date($(d).attr('added'));
            if (6 >= parseInt((dtCurrent.getTime() - dtRecord.getTime()) / (24 * 3600 * 1000)))
                $(d).addClass('imgtag');
        });

        var dates = [];
        var counter = 0;
        $('.collapsible[added]').each(function (i, d) {
            dates.push(new Date($(d).attr('added')));
        });
        $('#maxDate').text($.datepicker.formatDate('DD, d MM, yy', new Date(Math.max.apply(null, dates))));

        $('tr[added]').each(function (i, d) {
            var dtCurrent = new Date('@DateTime.Now.ToString("yyyy MM dd")');
            var dtRecord = new Date($(d).attr('added'));
            if (6 >= parseInt((dtCurrent.getTime() - dtRecord.getTime()) / (24 * 3600 * 1000))) {
                $(d).css("font-weight", "Bold");
                ++counter;
            }
        });
        if (counter > 0) $('#newCount').text(" (with " + counter + " new conventions)");
    }

    //$(processNew());
</script>
