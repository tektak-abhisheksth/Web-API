@using System.Linq
@using System.Web.Http.Description
@using Utility
@model API.Areas.HelpPage.Models.HelpPageApiModel

@{
    ApiDescription description = Model.ApiDescription;
}
<h1>
    @Model.ApiDescription.ActionDescriptor.ActionName.AddSpaceBeforeCapitalLetters()  @if (Model.RequiresAuthorization)
    {
        <img src="data:image/png;base64," style="vertical-align: sub" width="16" height="16" alt="Authorization" title="Requires authorized login token." />
    }
    else
    {
        <img src="data:image/png;base64," style="vertical-align: sub" width="16" height="16" alt="Temporary Token" title="Requires temporary token." />
    }
</h1>
<h4>HTTP Method: @description.HttpMethod.Method</h4>
API: <strong>@description.RelativePath</strong>
<p></p>
<p>@description.Documentation</p>
<div class="sample-header" style="text-transform: uppercase; box-shadow: 0 5px 8px rgba(0,0,0,0.5);"><strong>Request</strong></div>
<div style="border: 1px #00497E solid; padding: 5px 5px 5px 5px;box-shadow: 0 5px 8px rgba(0,0,0,0.5);">
    @if (Model.UriParameters != null && Model.UriParameters.Any())
    {
        <h3>URI Parameters</h3>
        @Html.DisplayFor(m => m.UriParameters, "Parameters")
    }

    @if (Model.RequestBodyParameters != null && Model.RequestBodyParameters.Any())
    {
        <h3>Body Parameters</h3>

        <p>@Model.RequestDocumentation</p>

        if (Model.RequestModelDescription != null)
        {
            <br />
            @*@Html.DisplayFor(m => m.RequestModelDescription.ModelType, "ModelDescriptionLink", new { modelDescription = Model.RequestModelDescription })*@
            if (Model.RequestBodyParameters != null)
            {
                //ViewBag.ModelName = Model.RequestModelDescription.ModelType.FullName;
                @Html.DisplayFor(m => m.RequestBodyParameters, "Parameters")
            }
        }
        else
        {
            <p>None.</p>
        }
    }
    @if (Model.SampleRequests.Count > 0)
    {
        <h3>Request Formats</h3>
        @Html.DisplayFor(m => m.SampleRequests, "Samples")
    }
</div>
<p>&nbsp;</p>
<div>
    <div class="sample-header" style="text-transform: uppercase;box-shadow: 0 5px 8px rgba(0,0,0,0.5);"><strong>Response</strong></div>
    <div style="border: 1px #00497E solid; padding: 5px 5px 5px 5px;box-shadow: 0 5px 8px rgba(0,0,0,0.5);">
        <p>@Html.Raw(string.IsNullOrWhiteSpace(description.ResponseDescription.Documentation) ? "The status of the operation." : description.ResponseDescription.Documentation)</p>
        @if (Model.ResourceDescription != null)
        {
            @*@Html.DisplayFor(m => m.ResourceDescription.ModelType, "ModelDescriptionLink", new { modelDescription = Model.ResourceDescription })*@
            if (Model.ResourceProperties != null)
            {
                <h3>Body Parameters</h3>
                @Html.DisplayFor(m => m.ResourceProperties, "Parameters")
            }
        }
        else
        {
            <p>None.</p>
        }

        @*@if (Model.SampleResponses.Count > 0)
            {
            <h3>Response Formats</h3>
            @Html.DisplayFor(m => m.SampleResponses, "Samples")
            }*@
    </div>
</div>
<p>&nbsp;</p>
<p>&nbsp;</p>
<div>
    @{
        var subject = string.Format(@"mailto:indira.sapkota@tektak.com?subject=About HTTP {0} method {1}&amp;cc=Abhisheksth@tektak.com", @description.HttpMethod.Method, @description.RelativePath);
    }
    Please report any bugs and/or changes to <a title="If your browser is setup for email, clicking here will let you send us an email." href="@subject">Web API team</a>.
</div>
<p>&nbsp;</p>
@{ Disqus.Initialize("iloopwebapi"); }
@Disqus.ShowComments(Model.ApiDescription.ActionDescriptor.GetCustomAttributes<Model.Attribute.MetaDataAttribute>().First().AliasName)